# Senha do painel admin (opcional; padrão: Nct0103@)
# Use o mesmo valor nos dois para o cadastro de produtos funcionar
# ADMIN_PASSWORD=sua_senha_segura
# NEXT_PUBLIC_ADMIN_PASSWORD=sua_senha_segura

# Supabase - Dashboard do projeto → Settings → API
# URL = Project URL
# Public (anon) = NEXT_PUBLIC_SUPABASE_ANON_KEY
# Secret (service_role) = SUPABASE_SERVICE_ROLE_KEY
# Cole os valores sem aspas. No Vercel: Settings → Environment Variables (mesmos nomes).
NEXT_PUBLIC_SUPABASE_URL=https://seu-projeto.supabase.co
SUPABASE_URL=https://seu-projeto.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Alternativa: JSONBin (sem banco, se não usar Supabase)
# JSONBIN_API_KEY=sua_api_key_aqui
# JSONBIN_BIN_ID=id_do_seu_bin
